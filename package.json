{
  "name": "todo-react",
  "version": "1.3.0",
  "private": true,
  "homepage": "http://amuou.github.io/todo-react",
  "dependencies": {
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.1.1",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
